{
    "plugins": [
        "stylelint-scss"
    ],
    "extends": [
        "stylelint-config-standard"
    ],
    "rules":{
        "at-rule-no-unknown": [ true, {
            "ignoreAtRules": [
              "include",
              "each",
              "extend"
            ]
          }],
          "number-leading-zero":null,
          "block-no-empty":null
    }
}